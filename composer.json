{
    "name": "philippe-vandermoere/docker-proxy",
    "description": "docker proxy http.",
    "type": "library",
    "homepage": "https://github.com/philippe-vandermoere/docker-proxy",
    "license": "MIT",
    "authors": [
        {
            "name": "Philippe VANDERMOERE",
            "email": "philippe@wizaplace.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "steevanb/php-typed-array": "^2.1",
        "symfony/console": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/yaml": "^5.0",
        "symfony/config": "^5.0",
        "twig/twig": "^2.9",
        "philippe-vandermoere/docker-php-sdk": "^1.3",
        "symfony/finder": "^5.0",
        "react/event-loop": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Test\\App\\": "tests/" }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "dev-master",
        "phpunit/phpunit": "^8.4",
        "fzaninotto/faker": "^1.9"
    }
}
